@model IEnumerable<ProductDefaultViewModel>

@if (Model.Count() > 0)
{
    @foreach (var product in Model)
    {
        <div class="col-lg-4 col-sm-6">
            <partial name="/Views/Products/Partials/_Product.cshtml" model="product" />
        </div>
    }
}
else
{
    <h3>Няма налични продукти в тази категория.</h3>
}